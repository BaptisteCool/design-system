{
	"name": "<%= name %>",
	"version": "0.1.0",
	"private": true,
	"description": "<%= description %>",
	"license": "UNLICENSED",
	"author": "<%= author %>",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"test:unit": "vue-cli-service test:unit",
		"test:unit:gui": "majestic"
	},
	"dependencies": {
		"@cnamts/vue-dot": "^2.0.0-alpha.20",
		"axios": "^0.19.0",
		"core-js": "^2.6.5",
		"dayjs": "^1.8.15",
		"languages": "^0.1.3",
		"vue": "^2.6.10",
		"vue-class-component": "^7.1.0",
		"vue-i18n": "^8.12.0",
		"vue-meta": "^2.2.0",
		"vue-router": "^3.0.7",
		"vuetify": "^2.0.4",
		"vuex": "^3.1.1",
		"vuex-persist": "^2.0.1"
	},
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/preset-env": "^7.5.5",
		"@mdi/js": "^3.8.95",
		"@types/jest": "^23.3.14",
		"@types/node": "^12.6.8",
		"@typescript-eslint/parser": "^1.13.0",
		"@vue/cli-plugin-babel": "^3.9.2",
		"@vue/cli-plugin-eslint": "^3.9.2",
		"@vue/cli-plugin-typescript": "^3.9.0",
		"@vue/cli-plugin-unit-jest": "^3.9.0",
		"@vue/cli-service": "^3.9.3",
		"@vue/eslint-config-standard": "^4.0.0",
		"@vue/eslint-config-typescript": "^4.0.0",
		"@vue/test-utils": "^1.0.0-beta.29",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "^10.0.2",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.2.3",
		"jest": "^23.6.0",
		"jest-serializer-vue": "^2.0.2",
		"lint-staged": "^9.2.1",
		"majestic": "^1.6.0",
		"sass": "^1.22.9",
		"sass-loader": "^7.1.0",
		"ts-jest": "^23.10.5",
		"typescript": "^3.5.3",
		"vue-cli-plugin-vuetify": "^0.6.1",
		"vue-template-compiler": "^2.6.10",
		"vuetify-loader": "^1.3.0",
		"webpack": "^4.38.0"
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"IE 11",
		"last 2 versions",
		"not ie <= 8"
	],
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"js",
			"json",
			"vue"
		],
		"transform": {
			"^.+\\.vue$": "vue-jest",
			".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
			"^.+\\.tsx?$": "ts-jest"
		},
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1",
			"^tests-unit/(.*)$": "<rootDir>/tests/unit/$1",
			"^vue$": "<rootDir>/node_modules/vue/dist/vue.common.js"
		},
		"snapshotSerializers": [
			"<rootDir>/node_modules/jest-serializer-vue"
		],
		"testMatch": [
			"<rootDir>/(tests/unit/**/*.spec.(js|ts)|**/__tests__/*.(js|ts))"
		],
		"coverageReporters": [
			"json",
			"lcov",
			"text",
			"html",
			"text-summary"
		],
		"coverageDirectory": "<rootDir>/tests/unit/coverage",
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.{ts,vue}",
			"!**/node_modules/**",
			"!**/*.d.ts",
			"!**/router.ts",
			"!**/main.ts",
			"!**/plugins/**",
			"!**/store/**",
			"!**/i18n.ts",
			"!**/services/**",
			"!**/tests/**",
			"!**/translations/**"
		],
		"testURL": "http://localhost/",
		"transformIgnorePatterns": [
			"./node_modules/(?!vuetify|vue-dot)"
		],
		"globals": {
			"ts-jest": {
				"babelConfig": true
			}
		}
	},
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.{ts,vue,js}": [
			"vue-cli-service lint",
			"git add"
		]
	},
	"majestic": {
		"jestScriptPath": "./node_modules/@vue/cli-service/bin/vue-cli-service.js",
		"args": [
			"test:unit"
		]
	}
}
