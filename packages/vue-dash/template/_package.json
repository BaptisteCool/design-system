{
	"name": "<%= name %>",
	"version": "0.1.0",
	"private": true,
	"description": "<%= description %>",
	"author": "<%= author %>",
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"test:unit": "vue-cli-service test:unit",
		"test:unit:gui": "majestic",
		"test:e2e": "vue-cli-service test:e2e"
	},
	"dependencies": {
		"@babel/polyfill": "^7.4.3",
		"@cnamts/vue-dot": "^1.6.2",
		"axios": "^0.18.0",
		"dayjs": "^1.8.12",
		"raven-js": "^3.27.0",
		"vue": "^2.6.10",
		"vue-i18n": "^8.10.0",
		"vue-meta": "^1.6.0",
		"vue-router": "^3.0.3",
		"vuetify": "^1.5.11",
		"vuetify-loader": "^1.2.1",
		"vuex": "^3.1.0",
		"vuex-persist": "^2.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.4.3",
		"@types/jest": "^24.0.11",
		"@types/node": "^11.13.4",
		"@types/promise.prototype.finally": "^2.0.3",
		"@typescript-eslint/parser": "^1.6.0",
		"@vue/cli-plugin-babel": "^3.5.5",
		"@vue/cli-plugin-eslint": "^3.5.1",
		"@vue/cli-plugin-typescript": "^3.5.3",
		"@vue/cli-plugin-unit-jest": "^3.5.3",
		"@vue/cli-service": "^3.5.3",
		"@vue/eslint-config-standard": "^4.0.0",
		"@vue/eslint-config-typescript": "^4.0.0",
		"@vue/test-utils": "^1.0.0-beta.29",
		"babel-core": "7.0.0-bridge.0",
		"babel-plugin-transform-imports": "^1.5.1",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.2.2",
		"jest": "^23.6.0",
		"jest-serializer-vue": "^2.0.2",
		"languages": "^0.1.3",
		"lint-staged": "^8.1.5",
		"majestic": "^1.3.0",
		"node-sass": "^4.11.0",
		"sass-loader": "^7.1.0",
		"stylus": "^0.54.5",
		"stylus-loader": "^3.0.2",
		"ts-jest": "^23.10.5",
		"typescript": "~3.2.1",
		"vue-cli-plugin-vuetify": "^0.5.0",
		"vue-template-compiler": "^2.6.10",
		"webpack": "^4.29.6"
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"IE 11",
		"last 2 versions",
		"not ie <= 8"
	],
	"jest": {
		"moduleFileExtensions": [
			"ts",
			"js",
			"json",
			"vue"
		],
		"transform": {
			"^.+\\.jsx?$": "babel-jest",
			"^.+\\.vue$": "vue-jest",
			".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
			"^.+\\.tsx?$": "ts-jest"
		},
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1",
			"^tests-unit/(.*)$": "<rootDir>/tests/unit/$1",
			"^vue$": "<rootDir>/node_modules/vue/dist/vue.common.js"
		},
		"snapshotSerializers": [
			"<rootDir>/node_modules/jest-serializer-vue"
		],
		"testMatch": [
			"<rootDir>/(tests/unit/**/*.spec.(js|ts)|**/__tests__/*.(js|ts))"
		],
		"coverageReporters": [
			"json",
			"lcov",
			"text",
			"html",
			"text-summary"
		],
		"coverageDirectory": "<rootDir>/tests/unit/coverage",
		"collectCoverage": true,
		"collectCoverageFrom": [
			"**/*.{ts,vue}",
			"!**/node_modules/**",
			"!**/*.d.ts",
			"!**/router.ts",
			"!**/main.ts",
			"!**/plugins/**",
			"!**/store/**",
			"!**/i18n.ts",
			"!**/services/**",
			"!**/tests/**",
			"!**/translations/**"
		],
		"testURL": "http://localhost/",
		"transformIgnorePatterns": [
			"./node_modules/(?!vuetify|vue-dot)"
		]
	},
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.ts": [
			"vue-cli-service lint",
			"git add"
		],
		"*.vue": [
			"vue-cli-service lint",
			"git add"
		],
		"*.js": [
			"vue-cli-service lint",
			"git add"
		]
	},
	"majestic": {
		"jestScriptPath": "./node_modules/@vue/cli-service/bin/vue-cli-service.js",
		"args": [
			"test:unit"
		]
	}
}
