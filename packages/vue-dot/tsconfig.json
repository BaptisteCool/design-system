{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"baseUrl": ".",
		"types": [
			"node",
			"jest"
		],
		"paths": {
			"vue-dot-src/*": [
				"src/*"
			]
		}
	},
	"include": [
		"playground/**/*.ts",
		"playground/**/*.vue",
		"src/**/*.ts",
		"src/**/*.vue",
		"tests/**/*.ts",
		"types/**/*.ts",
		"*.json"
	],
	"exclude": [
		"node_modules"
	]
}
